    "generate": "NODE_ENV=production stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^2.6.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.4.1",
    "@types/node": "^15.12.0",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.5",
    "tailwindcss": "^2.1.4"
  },
  "license": "MIT",
  "dependencies": {}
}
